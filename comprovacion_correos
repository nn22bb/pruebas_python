caracteres_aceptables = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789.-_"

def validar_email(email):
    if email.count("@") != 1:
        return False
    
    nombre, dominio = email.split("@")

    if not nombre or not dominio:
        return False

    if "." not in dominio or dominio.endswith("."):
        return False

    for c in nombre + dominio:
        if c not in caracteres_aceptables:
            return False

    return True


validos = []
invalidos = []

for correo in emails:
    if validar_email(correo):
        validos.append(correo)
    else:
        invalidos.append(correo)

print("Correos válidos:")
for c in validos:
    print("  -", c)

print("\n Correos inválidos:")
for c in invalidos:
    print("  -", c)